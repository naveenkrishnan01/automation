
require 'rubygems'
require 'pry'

class Url_check
    attr_accessor  :url 	
  def intialize
    @url = url	
  end


  def url_check_pt(url)
    if url != ""
      puts "The #{url}  was clicked"
    else
      puts " Url not found"
    end	 
  end

end  


